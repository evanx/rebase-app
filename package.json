      "example:delete": "node examples/delete",
      "react:dev": "cross-env NODE_ENV=development parcel examples/parcelReact/index.html ",
      "build": "cross-env NODE_ENV=production parcel build examples/parcelReact/index.html --no-minify --public-url /"
